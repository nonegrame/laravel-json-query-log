{
    "name": "nonerame/laravel-json-query-log",
    "description": "add database query log into JsonResponse",
    "license": "MIT",
    "authors": [
        {
            "name": "Nonegrame",
            "email": "nonegrame@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "^5.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Nonegrame\\LaravelJsonQueryLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nonegrame\\LaravelJsonQueryLog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nonegrame\\LaravelJsonQueryLog\\DatabaseQueryServiceProvider"
            ]
        }
    }
}
